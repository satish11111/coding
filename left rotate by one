import java.util.*;
public class Myclass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
	    int arr[]= {1,2,3,4,5};
	    int temp=arr[0];
	    for(int i=1;i<arr.length;i++) {
	    	arr[i-1]=arr[i];
	    }
	    arr[arr.length-1]=temp;
	   
	    
	    //
	    for(int x:arr) {
	    	System.out.println(x);
	    }
	}}
